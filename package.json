{
  "name": "atlas-guardrails",
  "version": "1.0.1",
  "description": "Atlas Guardrails CLI",
  "main": "dist/index.js",
  "bin": {
    "atlas": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.{ts,js}\"",
    "lint:fix": "eslint \"src/**/*.{ts,js}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "docs": "typedoc --out docs src",
    "prepare": "husky install",
    "prepublishOnly": "npm run build && npm run test && npm run lint"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "Marcus Gollahon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcusgoll/atlas-guardrails.git"
  },
  "bugs": {
    "url": "https://github.com/marcusgoll/atlas-guardrails/issues"
  },
  "homepage": "https://github.com/marcusgoll/atlas-guardrails#readme",
  "type": "commonjs",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@types/node": "^25.0.10",
    "better-sqlite3": "^12.6.2",
    "commander": "^14.0.2",
    "glob": "^13.0.0",
    "ignore": "^7.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "typedoc": "^0.28.16"
  }
}
